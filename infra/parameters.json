{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": { "value": "acmestore" },
    "location": { "value": "eastus" },
    "registryName": { "value": "acmestoreacr" },
    "dbAdminPassword": {
      "reference": {
        "keyVault": { "id": "/subscriptions/<subscription-id>/resourceGroups/<rg>/providers/Microsoft.KeyVault/vaults/<vault-name>" },
        "secretName": "db-admin-password"
      }
    },
    "dbAppPassword": {
      "reference": {
        "keyVault": { "id": "/subscriptions/<subscription-id>/resourceGroups/<rg>/providers/Microsoft.KeyVault/vaults/<vault-name>" },
        "secretName": "db-app-password"
      }
    },
    "jwtSecret": {
      "reference": {
        "keyVault": { "id": "/subscriptions/<subscription-id>/resourceGroups/<rg>/providers/Microsoft.KeyVault/vaults/<vault-name>" },
        "secretName": "jwt-secret"
      }
    }
  }
}
